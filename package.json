{
  "name": "cypress-cucumber-search",
  "version": "1.0.0",
  "description": "A Cypress framework for Behavior-Driven Development (BDD) using Cucumber for feature file integration and step definition mapping.",
  "scripts": {
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npm run clean:results && npm run clean:modules",
    "clean:modules": "rimraf ./node_modules",
    "clean:results": "rimraf ./cypress/reports ./cypress/videos ./cypress/screenshots ./cypress/downloads",
    "cy:open:prod": "npx cypress open --env ENV=prod",
    "cy:open:local": "npx cypress open --env ENV=local",
    "cy:run:prod": "npx cypress run --env ENV=prod",
    "cy:run:local": "npx cypress run --env ENV=local",
    "allure:generate": "allure generate cypress/reports/allure-results -o cypress/reports/allure-report --clean",
    "allure:generate:html": "allure generate --single-file cypress/reports/allure-results -o cypress/reports/allure-report-html --clean",
    "allure:open": "allure open cypress/reports/allure-report"
  },
  "keywords": [
    "Cypress",
    "JS",
    "JavaScript",
    "Cucumber",
    "Gherkin"
  ],
  "author": "pzhiznevsky",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/xpath": "^2.0.3",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@faker-js/faker": "^9.6.0",
    "allure-commandline": "^2.33.0",
    "allure-cypress": "^3.2.1",
    "allure-js-commons": "^3.1.1",
    "cypress": "^14.2.0",
    "cypress-on-fix": "^1.1.0",
    "cypress-split": "^1.24.14",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "uuid": "^11.1.0"
  }
}
